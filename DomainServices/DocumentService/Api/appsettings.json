{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "CIS": "Debug",
        "ExternalServices": "Debug",
        "DomainServices": "Debug",
        "CIS.Infrastructure.Security.CisServiceAuthenticationHandler": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
        "Microsoft.AspNetCore.Server.Kestrel": "Fatal"
      }
    }
  },

  "CisSecurity": {
    "ServiceAuthentication": {
      "Validator": "StaticCollection"
    }
  },

  "CisTelemetry": {
    "Logging": {
      "LogType": "Grpc",
      "Application": {
        "UseConsole": true,
        "File": {
          "Path": "r:\\LogFiles\\app\\DS-FAT\\DocumentService",
          "Filename": "DocumentService.log"
        },
        "Seq": {
          "ServerUrl": "http://localhost:6341"
        }
      },
      "Audit": {
        "File": {
          "Path": "r:\\LogFiles\\app\\DS-FAT\\DocumentService",
          "Filename": "Audit-DocumentService.log"
        }
      }
    }
  },

  "ExternalServices": {
    "ESignatures": {
      "V1": {
        "Authentication": "Basic",
        "ImplementationType": "Real",
        "UseServiceDiscovery": true,
        "Username": "login",
        "Password": "password"
      }
    }
  },

  "CisEnvironmentConfiguration": {
    "DefaultApplicationKey": "DS:DocumentService",
    "EnvironmentName": "FAT",
    "ServiceDiscoveryUrl": "https://172.30.35.51:5002",
    "InternalServicesLogin": "a",
    "InternalServicePassword": "a"
  }
}