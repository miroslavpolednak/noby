<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Company>Wookiee.Apps s.r.o.</Company>
		<Product>Common Infrastructure Solution</Product>
		<Authors>Filip Tuma</Authors>
		<LangVersion>preview</LangVersion>
		<Nullable>enable</Nullable>
		<Platforms>AnyCPU;x64</Platforms>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<PropertyGroup>
		<AnalysisMode>Recommended</AnalysisMode>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="CreateSalesArrangement.proto" />
		<None Remove="GetSalesArrangement.proto" />
		<None Remove="GetSalesArrangementByCaseId.proto" />
		<None Remove="LinkModelationToSalesArrangement.proto" />
		<None Remove="SalesArrangement.proto" />
		<None Remove="UpdateSalesArrangementState.proto" />
	</ItemGroup>

	<ItemGroup>
