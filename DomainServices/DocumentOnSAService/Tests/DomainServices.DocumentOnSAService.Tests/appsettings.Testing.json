{
  "CisEnvironmentConfiguration": {
    "EnvironmentName": "TEST",
    "SecretsSource": "Default",
    "ServiceDiscoveryUrl": "https://0.0.0.0",
    "InternalServicesLogin": "test",
    "InternalServicePassword": "Test",
    "DisableServiceDiscovery": true
  },

  "ConnectionStrings": {
    "default": "defaultCN"
  },

  "CisSecurity": {
    "ServiceAuthentication": {
      "Validator": "StaticCollection",
      "AllowedUsers": [
        {
          "Username": "test"
        }
      ]
    }
  },

  "CisTelemetry": {
    "Logging": {
      "Audit": {
        "HashSecretKey": "test"
      }
    }
  },
  "BackgroundServices": {
    "CheckDocumentsArchivedJob": {
      "CronSchedule": "* * * * *",
      "Disabled": true,
      "CustomConfiguration": {
        "MaxBatchSize": 1000
      }
    },
    "UpdateDocumentStatusJob": {
      "CronSchedule": "0 5 * * *",
      "Disabled": true,
      "CustomConfiguration": {
      }
    }
  }

}