{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Fatal",
      "Override": {
        "FOMS.LogApi": "Debug"
      }
    }
  },

  "CisTelemetry": {
    "Logging": {
      "Application": {
        "UseConsole": true,
        "File": {
          "Path": "r:\\LogFiles\\www\\NOBYFE-FAT",
          "Filename": "nobyfe_.log"
        }
      },
      "Audit": {
        "File": {
          "Path": "r:\\LogFiles\\www\\NOBYFE-FAT",
          "Filename": "Audit-nobyfe_.log"
        }
      }
    }
  },

  "CisEnvironmentConfiguration": {
    "DefaultApplicationKey": "FOMS:FE",
    "EnvironmentName": "FAT"
  },

  "CisCors": {
    "EnableCors": true,
    "AllowedOrigins": [ "https://noby-fat.vsskb.cz" ]
  }
}
