            <stringProp name="script">import groovy.json.JsonSlurper;

def response=prev.getResponseDataAsString();

def jsonSlurper=new JsonSlurper();
data = jsonSlurper.parseText(response);

def outputData=&apos;&apos;; 

def messageTypes=[:];

for(item in data)
{
  //outputData+=item.notificationId+&apos;\t\t&apos;+item.state+&apos;\n&apos;;

  if(!messageTypes.containsKey(item.documentId))
  {
	messageTypes[item.documentId]=[:];  	
  }
  
  if(messageTypes[item.documentId].containsKey(item.state))
  	messageTypes[item.documentId][item.state]+=1;
  else
  	messageTypes[item.documentId][item.state]=1;
}

def output=&apos;Results:\n\n&apos;;

messageTypes.each
{ 
	type, states -&gt; 
	output+=type+&apos;:\n&apos;
	states.each{ s, num -&gt; output+=s+&apos;: &apos;+num+&apos;\n&apos;;}
	output+=&apos;========================================\n\n&apos;
}

//Tohoto by bylo asi příliš:
//output+=&apos;========================================\n\n&apos;+outputData;


prev.setResponseData(output.getBytes(&apos;UTF-8&apos;));</stringProp>
