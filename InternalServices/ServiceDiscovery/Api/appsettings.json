{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "CIS": "Debug",
        "DomainServices": "Debug",
        "CIS.Infrastructure.Security.CisServiceAuthenticationHandler": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
        "Microsoft.AspNetCore.Server.Kestrel": "Fatal",
        "System.Net.Http.HttpClient": "Warning"
      }
    }
  },

  "ConnectionStrings": {
    "default": ""
  },

  "CisTelemetry": {
    "Logging": {
      "LogType": "Grpc",
      "Application": {
        "LogRequestPayload": true,
        "LogResponsePayload": true,
        "UseConsole": true,
        "File": {
          "Path": "r:\\LogFiles\\app\\DS-FAT\\ServiceDiscovery",
          "Filename": "ServiceDiscovery.log"
        },
        "Seq": {
          "ServerUrl": "https://localhost:6341"
        }
      }
    }
  },

  "CisEnvironmentConfiguration": {
    "DefaultApplicationKey": "CIS:ServiceDiscovery",
    "EnvironmentName": "FAT"
  }
}
