{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",

      "Override": {
        "CIS": "Verbose",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Verbose"
      }
    }
  },

  "ConnectionStrings": {
    "default": ""
  },

  "CisSecurity": {
    "ServiceAuthentication": {
      "Validator": "StaticCollection"
    }
  },

  "CisTelemetry": {
    "Logging": {
      "Application": {
        "UseConsole": true,
        "File": {
          "Path": "D:\\MPSS\\LogFiles\\NOBY",
          "Filename": "uat-caseservice.log"
        },
        "Seq": {
          "ServerUrl": "http://localhost:5341"
        }
      },
      "Audit": {
        "File": {
          "Path": "D:\\MPSS\\LogFiles\\NOBY",
          "Filename": "Audit-uat-caseservice.log"
        }
      }
    }
  },

  "CisEnvironmentConfiguration": {
    "DefaultApplicationKey": "CIS:DataAggregatorService",
    "EnvironmentName": "DEV",
    "InternalServicesLogin": "a",
    "InternalServicePassword": "a"
  },

  "DataAggregatorConfiguration": {
    "UseCacheForConfiguration": false
  }
}
