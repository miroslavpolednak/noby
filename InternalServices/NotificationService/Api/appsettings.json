{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "CIS": "Information",
        "ExternalServices": "Information",
        "DomainServices": "Information",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
        "Microsoft.AspNetCore.Server.Kestrel": "Fatal"
      }
    }
  },

  "ConnectionStrings": {
    "nobyDb": ""
  },

  "CisSecurity": {
    "ServiceAuthentication": {
      "Validator": "ActiveDirectory",
      "DomainUsernamePrefix": "vsskb\\",
      "AdHost": "vsskb.cz"
    }
  },

  "CisTelemetry": {
    "Logging": {
      "LogType": "Grpc",
      "Application": {
        "UseConsole": true,
        "File": {
          "Path": "r:\\LogFiles\\app\\DS-FAT\\NotificationService",
          "Filename": "NotificationService.log"
        },
        "Seq": {
          "ServerUrl": "http://localhost:6341"
        }
      },
      "Audit": {
        "File": {
          "Path": "r:\\LogFiles\\app\\DS-FAT\\NotificationService",
          "Filename": "Audit-NotificationService.log"
        }
      }
    }
  },

  "CisEnvironmentConfiguration": {
    "DefaultApplicationKey": "CIS:NotificationService",
    "EnvironmentName": "FAT",
    "ServiceDiscoveryUrl": "https://172.30.35.51:31000",
    "InternalServicesLogin": "",
    "InternalServicePassword": ""
  },

  "AppConfiguration": {
    "Consumers": [],
    "EmailSenders": {
      "Mcs": [],
      "Mpss": []
    },
    "EmailFormats": [],
    "EmailLanguageCodes": [],
