      "McsResult": "",
      "McsSender": "",
      "NobySendEmail": ""
    },
    "S3Buckets": {
      "Mcs": "",
      "Mpss": ""
    }
  },

  "S3Configuration": {
    "ServiceUrl": "https://ecs-s3.sos.kb.cz:9021",
    "AccessKey": "",
    "SecretKey": ""
  },

  "SmtpConfiguration": {
    "Host": "relay.mpss.cz",
    "Port": 25,
    "SecureSocket": "None"
  },

  "Apicurio": {
    "Url": "https://test.schema-registry.kbcloud/apis/registry/v2",
    "SchemaIdentificationType": "ContentId"
  },

  "AvroSerializer": {
    "SerializerType": "Confluent",
    "BufferBytes": 100,
    "SubjectNameStrategy": "Record",
    "AutoRegisterSchemas": false,
    "UseLatestVersion": false
  },

  "AvroDeserializer": {
    "DeserializerType": "Confluent"
  },

  "SpeedTracing": {
    "EamApplicationName": "NotificationService",
