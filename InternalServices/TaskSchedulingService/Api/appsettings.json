  },

  "FeatureManagement": {
    "BlueBang": true,
    "ElectronicSigning": true
  },

  "CisTelemetry": {
    "Logging": {
      "LogType": "Grpc",
      "Application": {
        "LogRequestPayload": true,
        "LogResponsePayload": true,
        "UseConsole": true,
        "File": {
          "Path": "",
          "Filename": "TaskSchedulerService.log"
        },
        "Seq": {
          "ServerUrl": ""
        }
      }
    },
    "Tracing": {
      "OTLP": {
        "CollectorUrl": ""
      }
    }
  },

  "ConnectionStrings": {
    "default": ""
  },

  "CisSecurity": {
    "ServiceAuthentication": {
      "Validator": "NativeActiveDirectory",
      "Domain": "vsskb.cz",
      "AdHost": "vsskb.cz",
      "AdPort": 636,
      "IsSsl": true,
      "AllowedUsers": [
        {
          "Username": "XX_NOBY_RMT_USR_TEST"
        }
      ]
    }
  },

  "ExternalServices": {
    "ESignatures": {
      "V1": {
        "ImplementationType": "Real",
        "UseServiceDiscovery": true,
        "Authentication": "Basic",
        "Username": "",
        "Password": "",
        "Tenant": "kb",
        "UseLogging": true,
        "IgnoreServerCertificateErrors": true,
        "RequestTimeout": 60
      }
    }
  }
